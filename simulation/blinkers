    %store/vec4 v0x14d621440_0, 0, 1;
    %load/vec4 v0x14d620e00_0;
    %store/vec4 v0x14d6212f0_0, 0, 4;
T_4.1 ;
    %jmp T_4;
    .thread T_4;
