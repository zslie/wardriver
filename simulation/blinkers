:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/va_math.vpi";
S_0x151805670 .scope module, "blinkers" "blinkers" 2 10;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rstbtn";
    .port_info 2 /OUTPUT 1 "led1";
    .port_info 3 /OUTPUT 1 "led2";
    .port_info 4 /OUTPUT 1 "led3";
v0x15181f5f0_0 .var "LED_1_BLINK_INTERVAL", 3 0;
v0x15181f6a0_0 .var "LED_2_BLINK_INTERVAL", 3 0;
o0x158040100 .functor BUFZ 1, C4<z>; HiZ drive
v0x15181f750_0 .net "clk", 0 0, o0x158040100;  0 drivers
v0x15181f800_0 .var "clk_edge", 0 0;
v0x15181f8a0_0 .var "cycle_count_led1", 3 0;
v0x15181f990_0 .var "cycle_count_led2", 3 0;
v0x15181fa40_0 .var "led1", 0 0;
v0x15181fae0_0 .var "led2", 0 0;
v0x15181fb80_0 .var "led3", 0 0;
o0x158040250 .functor BUFZ 1, C4<z>; HiZ drive
v0x15181fc90_0 .net "rstbtn", 0 0, o0x158040250;  0 drivers
E_0x151807990 .event posedge, v0x15181f800_0;
S_0x15180f5e0 .scope task, "cycle_led" "cycle_led" 2 38, 2 38 0, S_0x151805670;
 .timescale 0 0;
v0x15180f750_0 .var "blink_interval", 3 0;
v0x15181f4a0_0 .var "cycle_count", 3 0;
v0x15181f540_0 .var "led", 0 0;
TD_blinkers.cycle_led ;
    %load/vec4 v0x15181f4a0_0;
    %pad/u 32;
    %cmpi/u 0, 0, 32;
    %flag_or 5, 4; GT is !LE
    %flag_inv 5;
    %flag_get/vec4 5;
    %jmp/0 T_0.2, 5;
    %load/vec4 v0x15181f4a0_0;
    %pad/u 32;
    %load/vec4 v0x15180f750_0;
    %pad/u 32;
    %mod;
    %pushi/vec4 0, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
T_0.2;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x15181f4a0_0, 0, 4;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x15181f540_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %vpi_call 2 47 "$display", "[$display]: Cycle Task: Cycle Count: %d, Blink Interval: %d", v0x15181f4a0_0, v0x15180f750_0 {0 0 0};
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v0x15181f4a0_0;
    %pushi/vec4 1, 0, 4;
    %add;
    %store/vec4 v0x15181f4a0_0, 0, 4;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x15181f540_0, 0, 1;
T_0.1 ;
    %end;
    .scope S_0x151805670;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x15181fa40_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x15181fae0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x15181fb80_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x15181f800_0, 0, 1;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x15181f8a0_0, 0, 4;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x15181f990_0, 0, 4;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x15181f5f0_0, 0, 4;
    %pushi/vec4 5, 0, 4;
    %store/vec4 v0x15181f6a0_0, 0, 4;
    %end;
    .thread T_1;
    .scope S_0x151805670;
T_2 ;
    %vpi_call 2 19 "$dumpfile", "../vcd/blinkers.vcd" {0 0 0};
    %vpi_call 2 20 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x151805670 {0 0 0};
    %delay 1000, 0;
    %vpi_call 2 23 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x151805670;
T_3 ;
    %delay 1, 0;
    %delay 10, 0;
    %load/vec4 v0x15181f800_0;
    %inv;
    %store/vec4 v0x15181f800_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x151805670;
T_4 ;
    %wait E_0x151807990;
    %load/vec4 v0x15181fc90_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_4.0, 8;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x15181fa40_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x15181fae0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x15181fb80_0, 0, 1;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x15181f8a0_0, 0, 4;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x15181f990_0, 0, 4;
    %jmp T_4.1;
T_4.0 ;
    %load/vec4 v0x15181f5f0_0;
    %store/vec4 v0x15180f750_0, 0, 4;
    %load/vec4 v0x15181f8a0_0;
    %store/vec4 v0x15181f4a0_0, 0, 4;
    %fork TD_blinkers.cycle_led, S_0x15180f5e0;
    %join;
    %load/vec4 v0x15181f540_0;
    %store/vec4 v0x15181fa40_0, 0, 1;
    %load/vec4 v0x15181f4a0_0;
    %store/vec4 v0x15181f8a0_0, 0, 4;
    %load/vec4 v0x15181f6a0_0;
    %store/vec4 v0x15180f750_0, 0, 4;
    %load/vec4 v0x15181f990_0;
    %store/vec4 v0x15181f4a0_0, 0, 4;
    %fork TD_blinkers.cycle_led, S_0x15180f5e0;
    %join;
    %load/vec4 v0x15181f540_0;
    %store/vec4 v0x15181fae0_0, 0, 1;
    %load/vec4 v0x15181f4a0_0;
    %store/vec4 v0x15181f990_0, 0, 4;
T_4.1 ;
    %jmp T_4;
    .thread T_4;
